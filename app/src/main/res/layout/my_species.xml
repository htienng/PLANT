<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="412dp"
    android:layout_height="732dp"
    android:background="@drawable/borderlayout4">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="left"
        android:layout_marginStart="16dp"
        android:layout_marginTop="27dp"
        android:background="@drawable/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="152dp"
        android:layout_height="38dp"
        android:hint="My Species"
        android:textColor="#2D6936"
        android:textColorHint="#2D6936"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="20dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="364dp"
        android:layout_height="124dp"
        android:layout_gravity="left"
        android:background="@drawable/image12"
        android:foregroundTint="#9EFFE0E0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="103dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="364dp"
        android:layout_height="124dp"
        android:layout_gravity="left"
        android:background="@drawable/image13"
        android:foregroundTint="#9EFFE0E0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="248dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="364dp"
        android:layout_height="124dp"
        android:layout_gravity="left"
        android:background="@drawable/image14"
        android:foregroundTint="#9EFFE0E0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="395dp" />

    <Button
        android:layout_width="28dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="-65dp"
        android:layout_marginTop="-78dp"
        android:background="@drawable/pencil"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        tools:ignore="MissingConstraints" />


    <Button
        android:layout_width="28dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="-65dp"
        android:layout_marginBottom="-67dp"
        android:background="@drawable/pencil"
        app:layout_constraintBottom_toTopOf="@+id/imageView16"
        app:layout_constraintStart_toEndOf="@+id/imageView16"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button9"
        android:layout_width="28dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="-65dp"
        android:layout_marginBottom="-64dp"
        android:background="@drawable/pencil"
        app:layout_constraintBottom_toTopOf="@+id/imageView15"
        app:layout_constraintStart_toEndOf="@+id/imageView15"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView1"
        android:layout_width="444dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_navi_menu1">

        <FrameLayout
            android:id="@+id/container"
            android:layout_width="414dp"
            android:layout_height="50dp"></FrameLayout>
    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
