<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="412dp"
    android:layout_height="732dp"
    android:background="#CCFFCC"
    android:foregroundTint="#D7C8B5">

    <EditText
        android:id="@+id/editText10"
        android:layout_width="325dp"
        android:layout_height="206dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/borderlayout3"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="326dp"
        android:layout_height="154dp"
        android:layout_gravity="left"
        android:background="@drawable/image15"
        android:foregroundTint="#9EFFE0E0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="449dp" />
    <TextView
        android:id="@+id/textView23"
        android:layout_width="330dp"
        android:layout_height="54dp"
        android:layout_gravity="left"
        android:gravity="center"
        android:text="Even on Urban Excursions, Finding
Mother Nature's Charms"
        android:textColor="#000000"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="603dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="326dp"
        android:layout_height="245dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="22dp"
        android:background="@drawable/borderlayout3"
        app:layout_constraintBottom_toTopOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="323dp"
        android:layout_height="154dp"
        android:layout_gravity="left"
        android:background="@drawable/image14"
        android:foregroundTint="#9EFFE0E0"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="181dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="412dp"
        android:layout_height="126dp"
        android:layout_gravity="left"
        android:background="@drawable/image9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="334dp"
        android:layout_height="52dp"
        android:layout_gravity="left"
        android:gravity="center"
        android:text="David Austin, Who Breathed Life Into
the Rose, Is Dead at 92"
        android:textColor="#000000"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="335dp" />

    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="left"
        android:background="@drawable/avt"
        android:foregroundTint="#9EFFE0E0"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="394dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="66dp"
        android:layout_height="29dp"
        android:layout_gravity="left"
        android:background="@drawable/name"
        android:foregroundTint="#9EFFE0E0"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="394dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="57dp"
        android:layout_height="19dp"
        android:layout_gravity="left"
        android:background="@drawable/sh"
        android:foregroundTint="#9EFFE0E0"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="398dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="left"
        android:background="@drawable/back"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="90dp"
        android:layout_height="41dp"
        android:hint="Articles"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="28dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="45dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="329dp"
        android:layout_height="58dp"
        android:background="@drawable/borderlayout3"
        android:ems="15"
        android:hint="  Search For Plant"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="95dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_gravity="left"
        android:layout_marginStart="-45dp"
        android:layout_marginTop="-50dp"
        android:background="@drawable/search"
        app:layout_constraintStart_toEndOf="@+id/editText"
        app:layout_constraintTop_toBottomOf="@+id/editText" />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="444dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu">

        <FrameLayout
            android:id="@+id/container"
            android:layout_width="414dp"
            android:layout_height="50dp">

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black" />
        </FrameLayout>
    </com.google.android.material.bottomnavigation.BottomNavigationView>

</androidx.constraintlayout.widget.ConstraintLayout>
